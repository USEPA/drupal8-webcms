{#
/**
 * @file
 * Theme override to display a teaser node.
 */
#}

{% set more_info = [
  {
    'text': content.field_computed_comments_due_date|field_value,
    'label': content.field_computed_comments_due_date|field_label
  },
  {
    'text': content.field_geographic_locations|field_value,
    'label': content.field_geographic_locations|field_label
  }
] %}

{% include '@uswds/collection/_collection-item.twig' with {
  'url': url,
  'heading': label,
  'subtitle': content.field_secondary_headline,
  'more_info': more_info,
  'description': content.field_description,
} %}
{% set catch_cache = content|render %}
