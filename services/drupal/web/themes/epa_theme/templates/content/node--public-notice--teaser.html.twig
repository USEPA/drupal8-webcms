{#
/**
 * @file
 * Theme override to display a teaser node.
 */
#}

{% set metadata %}
  {{ content.field_computed_comments_due_date }}
  {{ content.field_geographic_locations }}
{% endset %}

{% set more_info = [
  {'text': content.field_computed_comments_due_date},
  {'text': content.field_geographic_locations}
] %}

{% include '@uswds/collection/_collection-item.twig' with {
  'url': url,
  'heading': label,
  'subtitle': content.field_secondary_headline,
  'more_info': more_info,
  'description': content.field_description,
} %}
{% set catch_cache = content|render %}
