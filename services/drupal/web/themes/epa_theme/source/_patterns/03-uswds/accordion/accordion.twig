<div class="usa-accordion">
{% for item in accordion %}
  <h2 class="usa-accordion__heading">
    <button class="usa-accordion__button" aria-expanded="false" aria-controls="a{{ loop.index }}">
      {{ item.title }}
    </button>
  </h2>
  <div id="a{{ loop.index }}" class="usa-accordion__content">
    {{ item.content }}
  </div>
{% endfor %}
</div>
