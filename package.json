{
  "name": "usepa-webcms8",
  "private": true,
  "scripts": {
    "dev": "nuxt --port 3003",
    "docker-dev": "nuxt --hostname '0' --port && eval $(grep DOCKER_DEV_PORT ./docker/dev/.env)",
    "clean:reports": "rm -rf mochawesome-report/",
    "merge:reports": "mochawesome-merge mochawesome-report/*.json > cypress-tests-report.json",
    "create:html:report": "marge --reportDir TestReport cypress-tests-report.json",
    "generate:html:report": "yarn run merge:reports && npm run create:html:report",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "e2e": "cypress run --config viewportWidth=1280,viewportHeight=720,specPattern=test/e2e/specs",
    "smoke": "cypress run --config viewportWidth=1280,viewportHeight=720,specPattern=test/e2e/specs/smoke",
    "smoke:dev": "yarn run cypress run --config viewportWidth=1280,viewportHeight=720,baseUrl=https://ccte-ccd-dev.epa.gov/dashboard,specPattern=test/e2e/specs/smoke",
    "smoke:stg": "yarn run cypress run --config viewportWidth=1280,viewportHeight=720,baseUrl=https://ccte-ccd-stg.epa.gov/dashboard,specPattern=test/e2e/specs/smoke",
    "smoke:uat": "yarn run cypress run --config viewportWidth=1280,viewportHeight=720,baseUrl=https://ccte-ccd.epa.gov/dashboard,specPattern=test/e2e/specs/smoke",
    "smoke:comptox": "yarn run cypress run --config viewportWidth=1280,viewportHeight=720,baseUrl=https://comptox.epa.gov/dashboard,specPattern=test/e2e/specs/smoke",
    "smoke:comptox-prod": "yarn run cypress run --config viewportWidth=1280,viewportHeight=720,baseUrl=https://comptox-prod.epa.gov/dashboard,specPattern=test/e2e/specs/smoke",
    "regression": "cypress run --config viewportWidth=1280,viewportHeight=720,specPattern=test/e2e/specs/regression",
    "regression:dev": "cypress run --config viewportWidth=1280,viewportHeight=720,specPattern=test/e2e/specs/regression,baseUrl=https://ccte-ccd-dev.epa.gov/dashboard",
    "regression:stg": "cypress run --config viewportWidth=1280,viewportHeight=720,specPattern=test/e2e/specs/regression,baseUrl=https://ccte-ccd-stg.epa.gov/dashboard",
    "regression:uat": "cypress run --config viewportWidth=1280,viewportHeight=720,specPattern=test/e2e/specs/regression,baseUrl=https://ccte-ccd.epa.gov/dashboard",
    "regression:comptox": "cypress run --config viewportWidth=1280,viewportHeight=720,specPattern=test/e2e/specs/regression,baseUrl=https://comptox.epa.gov/dashboard",
    "regression:comptox-prod": "cypress run --config viewportWidth=1280,viewportHeight=720,specPattern=test/e2e/specs/regression,baseUrl=https://comptox-prod.epa.gov/dashboard",
    "e2e-small": "cypress run --config viewportWidth=520,viewportHeight=720,specPattern=test/e2e/sm_specs",
    "e2e:open": "cypress open --config viewportWidth=1580,viewportHeight=720,specPattern=test/e2e/specs",
    "e2e:regression": "cypress open --config viewportWidth=1580,viewportHeight=720,specPattern=test/e2e/specs/regression",
    "e2e:smoke": "cypress run --config viewportWidth=1580,viewportHeight=720,specPattern=test/e2e/specs/smoke,baseUrl=http://localhost:3003/dashboard",
    "e2e:open-small": "cypress open --config viewportWidth=520,viewportHeight=720,specPattern=test/e2e/sm_specs",
    "test:e2e": "vue-cli-service test:e2e"
  },
